    "\n",
    "def mk_pct(row):\n",
    "    for col, pct_col in cols:\n",
    "        n = row[col]\n",
    "        try:\n",
    "            n = int(n)\n",
    "            row[pct_col] = row[col] / row.total_enrollment\n",
    "        except:\n",
    "            if \"Above\" in row[col]:\n",
    "                row[col] = np.ceil(row.total_enrollment * .96)\n",
    "                row[pct_col] = .96\n",
    "            elif \"Below\" in row[col]:\n",
    "                row[col] = np.ceil(row.total_enrollment * .04)\n",
    "                row[pct_col] = .04\n",
    "    return row\n",
    "\n",
    "df = df.apply(mk_pct, axis=1)\n",
    "df[[\"dbn\"] + pct_cols]"
   ]
